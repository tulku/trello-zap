from trello_zap import TrelloBackend


class TrelloZap(object):

    def __init__(self):
        backend = TrelloBackend()
        raw_materials = backend.load_data()
        for material in raw_materials:
            print material.create_timeline()


if __name__ == '__main__':
    tz = TrelloZap()


